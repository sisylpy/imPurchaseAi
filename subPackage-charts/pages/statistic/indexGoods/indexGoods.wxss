/* 供货商筛选器样式 */
.supplier-filter-container {
  padding: 20rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: center;
  align-items: center;
}

.supplier-filter-chip {
  display: inline-flex;
  align-items: center;
  justify-content: space-between;
  background-color: #187e6e;
  border-radius: 40rpx;
  padding: 16rpx 20rpx;
  width: fit-content;
  min-width: 200rpx;
}

.supplier-filter-text {
  font-size: 28rpx;
  color: #fff;
  flex: 1;
  white-space: nowrap;
}

.supplier-filter-delete {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 40rpx;
  height: 40rpx;
  margin-left: 16rpx;
  cursor: pointer;
}

.supplier-delete-icon {
  width: 24rpx;
  height: 24rpx;
  filter: brightness(0) invert(1); /* 将图标改为白色 */
}
.status.is-active{
  background: #1e82b4;
  border: none;
}
.conectLine{
  height: 8rpx;
