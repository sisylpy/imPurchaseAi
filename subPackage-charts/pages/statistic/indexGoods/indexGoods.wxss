@import "/template/resGoods/resGoods.wxss";
@import "/template/resGroup/resGroup.wxss";

/* 商品统计页面样式 */
.goods-card {
  background-color: white;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
  padding: 30rpx;
  box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.1);
}

.card-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}

.category-icon {
  width: 60rpx;
  height: 60rpx;
  border-radius: 12rpx;
  margin-right: 20rpx;
}

.category-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.purchase-summary {
  font-size: 28rpx;
  color: #333;
  font-weight: 500;
}

.button-row {
  display: flex;
  justify-content: space-between;
  gap: 20rpx;
}

.data-button {
  flex: 1;
  height: 80rpx;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
}

.purchase-btn {
  background-color: #007aff;
}

.expense-btn {
  background-color: #ff6b6b;
}

.inventory-btn {
  background-color: #6f42c1;
}

.button-text {
  color: rgb(240, 240, 240);
  font-size: 26rpx;
  /* font-weight: 500; */
}



/*  */

.donut-chart-container {
  margin-left: 20rpx;
  flex-shrink: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.debug-info {
  margin-top: 8rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.debug-text {
  font-size: 18rpx;
  color: #999;
  line-height: 1.2;
}

.donut-chart {
  border-radius: 50%;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}


.donut-percent {
  font-size: 18rpx;
  font-weight: bold;
  color: #333;
  z-index: 2;
  position: relative;
  background-color: white;
  width: 60%;
  height: 60%;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* 定义hover效果，覆盖内联样式 */
.hover {
  background-color: #f0f0f0 !important;
}

