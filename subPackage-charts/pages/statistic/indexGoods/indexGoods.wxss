  /* padding: 10rpx 20rpx; */

}

.navbar .navbar-item {
  position: relative;
  display: block;
  font-size: 30rpx;
  color: #425042;
  
}

.navbar .bar-item-on {
  color: #187e6e;
  
}

.navbar .navbar-title {
  font-size: 38rpx;
}

.navbar .bar-item-on .navbar-title {
  color: #187e6e;
