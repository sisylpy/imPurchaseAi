  color: #1ba0e5;

}

.navbar .navbar-title {
  display: inline-block;
  font-size: 32rpx;
  width: auto;


}

.navbar .bar-item-on .navbar-title {
  color: #1ba0e5(236, 233, 233);
  font-weight: 600;
  /* border-bottom: 5rpx solid #1ba0e5; */
}

.navbar .navbar-slider {
  /* position: absolute; */
  content: " ";
  left: 0;
  bottom: 0;
  width: 100rpx;
  height: 5rpx;
  background-color: #1ba0e5;
  /* border-bottom: 1px solid lightgray; */
  /* box-shadow: 0 0 0 2rpx #b8b9b981; */

  -webkit-transition: -webkit-transform 0.3s;
  transition: -webkit-transform 0.3s;
  transition: transform 0.3s, -webkit-transform 0.3s;
}


.dotCar {
  position: absolute;
  width: 34rpx;
