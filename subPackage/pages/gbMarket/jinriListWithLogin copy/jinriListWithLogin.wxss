



.settleBtn{
  padding: 10rpx 20rpx;
  border-radius: 6rpx;
  /* font-size: 38rpx; */
}

.containerIndex {
  width: 100%;
  height: 100%;
  position: relative;
  overflow: hidden;
}

.side-menu {
  height: 100%;
  position: absolute;
  top: 0;
  z-index: 11000;
}

.menu-content {
  padding: 20rpx;
  color: #fff;
}

.main {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1000;
}

.overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10500;
  background-color: rgba(0, 0, 0, 0.5);

}

.top-bar {
  height: 100rpx;
  background-color: #eee;
  display: flex;
  align-items: center;
}

.menu-button {
  padding: 20rpx;
}


.selBack{
  background-color: rgb(255, 255, 255);
  color: #000;
}


.new_btn{
  padding: 4rpx 20rpx;
  background-color: #fff;
  color: #1ba0e5;
  font-size: 30rpx;
  /* font-weight: 500; */
  border-radius: 40rpx;
  margin-top: 20rpx;
  border: 1px solid #1ba0e5;
}
