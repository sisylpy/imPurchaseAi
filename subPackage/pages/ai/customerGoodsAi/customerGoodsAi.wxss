



.ai-loading {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh;
  background-color: #fff6df;
}

.ai-bubble {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.ai-icon {
  width: 120rpx;
  height: 120rpx;
  margin-bottom: 20rpx;
}

.loading-text {
  font-size: 28rpx;
  color: #999;
}




.goods_url{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  
}

.bg_back{
  background-color: #fff6df;

}

.empty-bg {
  background-image: url('https://grainservice.club:8443/nongxinle/uploadImage/ChatLogo3.png');
  background-size: cover;
  background-repeat: no-repeat;
}

.goods_back{
  background-color: rgb(255, 251, 242);
  border: 2px solid rgb(214, 210, 191);
  border-radius: 30rpx;
}

.save_btn{
  background-color: #f08308;
  color: #fff;
  font-size: 32rpx;
  font-weight: 500;
  border-radius: 20rpx;
  padding: 10rpx 40rpx;
}